def primeno( num):
    if(num==1 or num == 2 or num == 3 or num== 5 ):
        return True
    if(num%2 ==0):
        return False
    if(num%3 == 0):
        return False
    if(num%5 == 0):
        return False

num = int(input("enter number for judginh the prime number "))
if(primeno(num) == False):
    print("not prime number")
else:
    print("prime number ")
